{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      // Temporarily enabling HTTP/1 due to https://github.com/prometheus/prometheus/issues/7237
      "Protocols": "Http1AndHttp2"
    }
  },
  "Sentry": {
    "MaxRequestBodySize": "medium",
    "SendDefaultPii": true,
    "TracesSampleRate": 1.0
  }
}
